{
    "name": "yieldstudio/nova-google-polygon",
    "description": "A Laravel Nova Google polygon field.",
    "keywords": [
        "yieldstudio",
        "laravel",
        "nova",
        "google",
        "polygon",
        "area"
    ],
    "homepage": "https://github.com/YieldStudio/nova-google-polygon",
    "license": "MIT",
    "authors": [
        {
            "name": "James Hemery",
            "email": "james@yieldstudio.fr",
            "homepage": "https://yieldstudio.fr",
            "role": "Developer"
        }
    ],
    "repositories": [
        {
            "type": "composer",
            "url": "https://nova.laravel.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "laravel/nova": "^4.0|^5.0",
        "illuminate/support": "^8|^9|^10|^11|^12",
        "ext-bcmath": "*"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.8",
        "pestphp/pest": "^1.21",
        "phpunit/phpunit": "^9.1|^10|^11|^12"
    },
    "autoload": {
        "psr-4": {
            "YieldStudio\\NovaGooglePolygon\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "YieldStudio\\NovaGooglePolygon\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "YieldStudio\\NovaGooglePolygon\\FieldServiceProvider"
            ]
        }
    },
    "scripts": {
        "format": "vendor/bin/php-cs-fixer fix --allow-risky=yes",
        "test": "vendor/bin/pest"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
